{
  "diagnostic.errorSign": "✘",
  "diagnostic.infoSign": "ℹ",
  "diagnostic.hintSign": "➤",
  "highlight.colors.enable": false,
  "hover.target": "float",
  "languageserver": {
   
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.work", "go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }

  }
}
